%%LOAD DATASET...
disp('Loading set file: [batch_dfn]...');
EEG = pop_loadset('filename','[batch_dfn]','filepath','[batch_dfp]');
EEG = eeg_checkset( EEG );

%%INITIATE MARKS...
EEG.marks=marks_init(size(EEG.data),0);
EEG.marks=marks_add_label(EEG.marks,'time_info', ...
	{'type-1',[1,0,0],zeros(1,EEG.pnts)});
EEG.marks=marks_add_label(EEG.marks,'time_info', ...
	{'type-2',[0,1,0],zeros(1,EEG.pnts)});
EEG.marks=marks_add_label(EEG.marks,'time_info', ...
	{'type-3',[1,1,0],zeros(1,EEG.pnts)});
EEG.marks=marks_add_label(EEG.marks,'time_info', ...
	{'type-4',[0,0,1],zeros(1,EEG.pnts)});
EEG.marks=marks_add_label(EEG.marks,'time_info', ...
	{'type-5',[1,0,1],zeros(1,EEG.pnts)});
EEG.marks=marks_add_label(EEG.marks,'time_info', ...
	{'type-6',[0,1,1],zeros(1,EEG.pnts)});
EEG.marks=marks_add_label(EEG.marks,'time_info', ...
	{'thing-q',[1,.5,.5],zeros(1,EEG.pnts)});
EEG.marks=marks_add_label(EEG.marks,'time_info', ...
	{'thing-w',[.5,1,.5],zeros(1,EEG.pnts)});
EEG.marks=marks_add_label(EEG.marks,'time_info', ...
	{'thing-e',[1,1,.5],zeros(1,EEG.pnts)});


%%ANNOTATE THE DATA...
EEG=pop_vised(EEG,'pop_gui','off');
uiwait;

%%SAVE *scr.set FILE...
EEG=pop_saveset(EEG,'filename','[batch_dfn,.,-1]_an.set','filepath','[batch_dfp]');

